{
    "name": "health-use-computer",
    "version": "1.0.0",
    "private": true,
    "main": "electron/main.ts",
    "scripts": {
        "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\" \"npm run start-electron\"",
        "start-backend": "cd backend && uvicorn main:app --reload --host localhost --port 8000 --ws websockets",
        "start-frontend": "cd frontend && npm run dev",
        "start-electron": "electron electron/main.ts"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "electron": "^15.3.0"
    },
    "dependencies": {
        "@ant-design/charts": "^2.2.6",
        "dayjs": "^1.11.13",
        "react-router-dom": "^7.1.5",
        "use-sound": "^5.0.0"
    }
}
